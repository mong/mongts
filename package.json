    "@types/react": "18.0.19",
    "babel-plugin-istanbul": "6.1.1",
    "cypress": "10.7.0",
    "eslint": "8.23.0",
    "eslint-config-next": "12.3.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "start-server-and-test": "1.14.0",
    "typescript": "4.8.3"
