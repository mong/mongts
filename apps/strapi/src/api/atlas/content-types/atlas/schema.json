{
  "kind": "collectionType",
  "collectionName": "atlases",
  "info": {
    "singularName": "atlas",
    "pluralName": "atlases",
    "displayName": "Atlas",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "publisert": {
      "type": "boolean",
      "default": false
    },
    "date": {
      "type": "datetime"
    },
    "mainTitle": {
      "type": "string"
    },
    "shortTitle": {
      "type": "string"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "frontpagetext": {
      "type": "text",
      "required": true
    },
    "ingress": {
      "type": "richtext"
    },
    "lang": {
      "type": "enumeration",
      "enum": ["nb", "nn", "en"]
    },
    "kapittel": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::kapittel.kapittel"
    },
    "jsondata": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::atlas-data-json.atlas-data-json"
    }
  }
}
