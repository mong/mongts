{
  "name": "qmongjs",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "types": "./src/index.tsx",
  "scripts": {
    "test": "yarn run test:base --coverage",
    "test:base": "react-scripts test --watchAll=false",
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx src",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|html|yml|yaml|css|md)\"",
    "format": "yarn run prettier --write",
    "check-format": "yarn run prettier --check",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/d3": "7.4.0",
    "@types/d3-format": "3.0.1",
    "@types/react": "18.0.27",
    "@types/react-select": "5.0.1",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "eslint": "8.33.0",
    "next-query-params": "4.1.0",
    "next-router-mock": "0.8.0",
    "prettier": "2.8.3",
    "react-dom": "18.2.0",
    "react-markdown": "8.0.5",
    "react-scripts": "5.0.1",
    "remark-gfm": "3.0.1",
    "types": "*",
    "typescript": "4.9.4"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "5.11.6",
    "@visx/event": "3.0.0",
    "@visx/tooltip": "3.0.0",
    "d3": "7.8.2",
    "d3-format": "3.1.0",
    "next": "13.1.5",
    "react": "18.2.0",
    "react-icons": "4.7.1",
    "react-query": "3.39.3",
    "react-select": "5.7.0",
    "resize-observer-polyfill": "1.5.1",
    "use-query-params": "^2.1.1"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "statements": 3,
        "branches": 4,
        "functions": 1,
        "lines": 3
      }
    },
    "transformIgnorePatterns": [
      "<rootDir>/../../node_modules/remark-gfm/"
    ],
    "moduleNameMapper": {
      "d3": "<rootDir>/../../node_modules/d3/dist/d3.min.js",
      "react-markdown": "<rootDir>/../../node_modules/react-markdown/react-markdown.min.js"
    }
  }
}
