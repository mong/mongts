{
  "name": "qmongjs",
  "version": "0.0.0",
  "main": "./src/index.tsx",
  "types": "./src/index.tsx",
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --ext js,ts,tsx src",
    "prettier": "prettier \"**/*.+(js|jsx|ts|tsx|json|html|yml|yaml|css|md)\"",
    "format": "yarn run prettier --write",
    "check-format": "yarn run prettier --check",
    "check-types": "tsc --noEmit",
    "test": "jest --watchAll=false --coverage"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "coverageThreshold": {
      "global": {
        "statements": 3,
        "branches": 4,
        "functions": 1,
        "lines": 3
      }
    },
    "moduleNameMapper": {
      "d3": "<rootDir>/node_modules/d3/dist/d3.min.js",
      "react-markdown": "<rootDir>/node_modules/react-markdown/react-markdown.min.js"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/d3": "7.4.0",
    "@types/d3-format": "^3.0.1",
    "@types/jest": "29.0.3",
    "@types/node": "18.0.0",
    "@types/react": "18.0.23",
    "babel-jest": "^29.2.2",
    "babel-plugin-istanbul": "6.1.1",
    "eslint": "8.26.0",
    "is-ci-cli": "2.2.0",
    "jest": "^29.2.2",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "13.0.3",
    "msw": "0.47.3",
    "next-router-mock": "^0.7.4",
    "prettier": "2.7.1",
    "react-markdown": "8.0.3",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.0.3",
    "typedoc": "0.23.15",
    "typescript": "4.8.3"
  },
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.6",
    "@mui/material": "5.10.6",
    "@visx/event": "2.6.0",
    "@visx/tooltip": "2.10.0",
    "d3": "7.6.1",
    "d3-format": "3.1.0",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-icons": "4.4.0",
    "react-query": "3.39.2",
    "react-select": "5.4.0",
    "remark-gfm": "3.0.1",
    "resize-observer-polyfill": "1.5.1",
    "source-map-explorer": "2.5.2",
    "use-query-params": "^2.1.1"
  }
}
